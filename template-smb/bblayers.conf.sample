# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ##OEROOT##/layers/poky/meta \
  ##OEROOT##/layers/poky/meta-poky \
  ##OEROOT##/layers/poky/meta-yocto-bsp \
  ##OEROOT##/layers/meta-openembedded/meta-oe \
  ##OEROOT##/layers/meta-openembedded/meta-networking \
  ##OEROOT##/layers/meta-openembedded/meta-python \
  ##OEROOT##/layers/meta-connectedway \
  "
